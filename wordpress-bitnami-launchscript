sudo wp plugin update --all
sudo wp theme update --all
sudo wp plugin install woocommerce --activate
sudo wp plugin install classic-editor  --activate
sudo wp theme install generatepress --activate
wget https://raw.githubusercontent.com/jxm90/wordpress-resources/main/security-functions.txt
sudo chmod 666 /bitnami/wordpress/wp-content/themes/generatepress/functions.php
cat security-functions.txt >> /bitnami/wordpress/wp-content/themes/generatepress/functions.php
sudo chmod 644 /bitnami/wordpress/wp-content/themes/generatepress/functions.php
sudo wp core update
